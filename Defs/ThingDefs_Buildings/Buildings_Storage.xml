<?xml version="1.0" encoding="utf-8" ?>
<Buildings>

	  <!-- Nesting Box -->

	<ThingDef ParentName="FurnitureBase">
		<defName>CCPSS_NestingBox</defName>
		<ThingClass>Building_Storage</ThingClass>
		<label>nesting box</label>
		<description>A nesting box for animal eggs.</description>
		<rotatable>true</rotatable>
		<fillPercent>0.25</fillPercent>
		<PathCost>40</PathCost>
		<CastEdgeShadows>false</CastEdgeShadows>
		<Passability>PassThroughOnly</Passability>
		<graphicData>
			<texPath>Storage/NestingBox</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
      <damageData>
        <rect>(0.05,0.05,0.9,0.9)</rect>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
		</graphicData>
	 <building>
	  <ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
      <preventDeterioration>true</preventDeterioration>
      <fixedStorageSettings>
        <filter>
          <categories>
            <li>EggsUnfertilized</li>
            <li>EggsFertilized</li>
          </categories>
        </filter>
      </fixedStorageSettings>
      <defaultStorageSettings>
        <priority>Critical</priority>
        <filter>
          <categories>
            <li>EggsFertilized</li>
		  </categories>
        </filter>
      </defaultStorageSettings>
	  <isSittable>true</isSittable>
    </building>
	<socialPropernessMatters>true</socialPropernessMatters>
	<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
	</stuffCategories>
	<costStuffCount>45</costStuffCount>
	<AltitudeLayer>Building</AltitudeLayer>
	<statBases>
			<Beauty>1</Beauty>
			<MaxHitPoints>140</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<WorkToBuild>650</WorkToBuild>
      <Mass>10</Mass>
	</statBases>
	<comps>
			<li Class="CompProperties_RoomIdentifier">
				<roomStat>Impressiveness</roomStat>
			</li>
	</comps>
	<inspectorTabs>
      <li>ITab_Storage</li>
    </inspectorTabs>
    <designationCategory>Furniture</designationCategory>
    <surfaceType>Item</surfaceType>
	</ThingDef>

